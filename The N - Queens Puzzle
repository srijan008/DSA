#include <bits/stdc++.h>

void pushSolution(int n, vector<int> store, vector<vector<int>> map,
     vector<vector<int>> &ans){
	vector<int> store;
	for(int i =0 ;i<n; i++){
		for(int j =0; j<n; j++){
			store.push_back(map[i][j]);
		}
	}
	ans.push_back(store);
}

void solve(int col,vector<vector<int>> map,vector<vector<int>> &ans){
	if(col == n){
		pushSolution(n, store, map);
		return;
	}
	for(int row = 0; row<n; row++){
		if(isSafe()){
			map[i][j] = 1;
			solve(col+1, map, ans);
			map[i][j] = 0;
		}
		
	}
}
bool isSafe(){
	int x = row;
	int y = col;
	while()
}
